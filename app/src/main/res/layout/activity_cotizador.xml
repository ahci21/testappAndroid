<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#f5f5f5"

        tools:context=".Cotizador">
    <androidx.cardview.widget.CardView
            android:layout_width="146dp"
            android:layout_height="168dp"
            app:cardCornerRadius="10dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="40dp"
            android:id="@+id/cardView3"
            android:layout_marginTop="60dp" app:layout_constraintTop_toBottomOf="@+id/cardView2"/>
    <androidx.cardview.widget.CardView
            android:layout_width="146dp"
            android:layout_height="168dp"
            app:cardCornerRadius="10dp" app:layout_constraintEnd_toStartOf="@+id/cardView3"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.734"
            android:layout_marginTop="60dp" app:layout_constraintTop_toBottomOf="@+id/cardView2"
            android:id="@+id/cardView5"/>
    <androidx.cardview.widget.CardView
            android:layout_width="375dp"
            android:layout_height="231dp"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:id="@+id/cardView2" app:cardBackgroundColor="#03a9f5"
            app:cardCornerRadius="20dp">
        <TextView
                android:text="Ganancia"
                android:layout_width="394dp"
                android:layout_height="40dp"
                android:id="@+id/textView3"
                android:textSize="24sp"
                android:textAlignment="viewStart"
                android:background="#000077C2" android:textColor="#ffffff"
        />
    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
            android:layout_width="393dp"
            android:layout_height="119dp"
            android:layout_marginBottom="104dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:id="@+id/cardView4" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/cardView3" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintVertical_bias="0.619"/>
</androidx.constraintlayout.widget.ConstraintLayout>