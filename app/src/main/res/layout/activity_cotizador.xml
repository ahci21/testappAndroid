<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#f5f5f5"

        tools:context=".Cotizador">
    <androidx.cardview.widget.CardView
            android:layout_width="146dp"
            android:layout_height="168dp"
            app:cardCornerRadius="10dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="32dp"
            android:id="@+id/cardView3"
            android:layout_marginTop="12dp" app:layout_constraintTop_toBottomOf="@+id/cardView2"
            app:cardBackgroundColor="#ad4cf8"/>
    <androidx.cardview.widget.CardView
            android:layout_width="146dp"
            android:layout_height="168dp"
            app:cardCornerRadius="10dp" app:layout_constraintEnd_toStartOf="@+id/cardView3"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.666"
            app:layout_constraintTop_toBottomOf="@+id/cardView2"
            android:id="@+id/cardView5" app:cardBackgroundColor="#644cf8" android:layout_marginTop="12dp">
    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
            android:layout_width="368dp"
            android:layout_height="0dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:id="@+id/cardView2" app:cardBackgroundColor="#03a9f5"
            app:cardCornerRadius="20dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp" android:layout_marginBottom="188dp"
            app:layout_constraintBottom_toTopOf="@+id/cardView4">
        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
        >

            <TextView
                    android:text="Ganancia"
                    android:layout_width="361dp"
                    android:layout_height="31dp" android:id="@+id/textView6"


                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="8dp"
                    app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.444"
                    android:textColor="#ffffff" android:textSize="18sp" android:paddingLeft="20dp"
                    android:paddingTop="5dp"/>
            <TextView
                    android:text="$00.00"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/txtGanancia" android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@+id/textView6" android:layout_marginBottom="8dp"
                    app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginEnd="8dp"
                    android:textColor="#ffffffff" android:textSize="50sp"
                    app:layout_constraintVertical_bias="0.317"
                    android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"/>

        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
            android:layout_width="393dp"
            android:layout_height="0dp"
            android:layout_marginBottom="81dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:id="@+id/cardView4" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/cardView2" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.0" app:cardBackgroundColor="#f8ea4c"/>
</androidx.constraintlayout.widget.ConstraintLayout>