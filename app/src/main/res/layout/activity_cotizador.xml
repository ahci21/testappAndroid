<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        tools:context=".Cotizador">
    <androidx.cardview.widget.CardView
            android:layout_width="146dp"
            android:layout_height="168dp"
            app:cardCornerRadius="10dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="32dp"
            android:id="@+id/cardView3" android:layout_marginTop="28dp"
            app:layout_constraintTop_toBottomOf="@+id/textView3"/>
    <androidx.cardview.widget.CardView
            android:layout_width="146dp"
            android:layout_height="168dp"
            app:cardCornerRadius="10dp" app:layout_constraintEnd_toStartOf="@+id/cardView3"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintHorizontal_bias="0.541"
            android:layout_marginBottom="44dp" app:layout_constraintBottom_toTopOf="@+id/cardView4"
            android:layout_marginTop="28dp" app:layout_constraintTop_toBottomOf="@+id/textView3"/>
    <androidx.cardview.widget.CardView
            android:layout_width="396dp"
            android:layout_height="222dp"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:id="@+id/cardView2"/>
    <TextView
            android:text="Ganancia"
            android:layout_width="386dp"
            android:layout_height="34dp"
            android:id="@+id/textView3" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp" android:textSize="24sp"
            android:textAlignment="viewEnd" app:layout_constraintTop_toBottomOf="@+id/cardView2"
            app:layout_constraintHorizontal_bias="1.0" android:layout_marginTop="8dp"/>
    <androidx.cardview.widget.CardView
            android:layout_width="393dp"
            android:layout_height="119dp"
            android:layout_marginTop="44dp"
            app:layout_constraintTop_toBottomOf="@+id/cardView3" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintVertical_bias="0.0" android:id="@+id/cardView4"/>
</androidx.constraintlayout.widget.ConstraintLayout>